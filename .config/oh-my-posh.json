{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": false,
      "segments": [
        {
          "foreground": "#BBC4B9",
          "properties": {
            "time_format": "3:04 PM"
          },
          "style": "diamond",
          "template": "{{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "foreground": "#BFA8BF",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "diamond",
          "template": "<#fff>{{ .UpstreamIcon }}</>  {{ .HEAD }}",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#fff",
          "style": "plain",
          "type": "text"
        },
        {
          "properties": {
            "root_icon": "\uf292 "
          },
          "style": "diamond",
          "template": " \uf0e7",
          "type": "root"
        },
        {
          "foreground": "#BBC4B9",
          "style": "diamond",
          "properties": {
            "extensions": [
              "*.js",
              "*.ts",
              "package.json",
              ".nvmrc",
              "pnpm-workspace.yaml",
              ".pnpmfile.cjs",
              ".vue"
            ]
          },
          "template": " <#fff>\ue718</> {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": false,
      "segments": [
        {
          "foreground": "#7DAEA3",
          "properties": {
            "folder_icon": "\u002F",
            "folder_separator_icon": "<#FFB300>\uf460 </>",
            "home_icon": "~",
            "style": "agnoster_full"
          },
          "style": "diamond",
          "template": "{{ .Path }}",
          "trailing_diamond": "",
          "type": "path"
        },
        {
          "foreground": "#89B482",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#fff{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " $ ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "transient_prompt": {
    "background": "transparent",
    "template": "<#89B482>$</> "
  },
  "version": 3
}
